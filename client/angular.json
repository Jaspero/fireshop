{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shop": {
      "root": "apps/shop",
      "sourceRoot": "apps/shop/src",
      "projectType": "application",
      "prefix": "jfs",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss",
          "changeDetection": "OnPush"
        },
        "@schematics/angular:directive": {
          "flat": false
        },
        "@schematics/angular:pipe": {
          "flat": false
        },
        "@schematics/angular:service": {
          "flat": false
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../dist/public/shop",
            "index": "apps/shop/src/index.html",
            "main": "apps/shop/src/main.ts",
            "polyfills": "apps/shop/src/polyfills.ts",
            "tsConfig": "apps/shop/src/tsconfig.app.json",
            "assets": [
              "apps/shop/src/favicon.ico",
              "apps/shop/src/assets",
              "apps/shop/src/manifest.json"
            ],
            "styles": ["apps/shop/src/styles.scss"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/shop/src/environments/environment.ts",
                  "with": "apps/shop/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "shop:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "shop:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "shop:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/shop/src/test.ts",
            "polyfills": "apps/shop/src/polyfills.ts",
            "tsConfig": "apps/shop/src/tsconfig.spec.json",
            "karmaConfig": "apps/shop/src/karma.conf.js",
            "styles": ["apps/shop/src/styles.scss"],
            "scripts": [],
            "assets": [
              "apps/shop/src/favicon.ico",
              "apps/shop/src/assets",
              "apps/shop/src/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/shop/src/tsconfig.app.json",
              "apps/shop/src/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "../dist/server",
            "main": "apps/shop/src/main-ssr.ts",
            "tsConfig": "apps/shop/src/tsconfig.app-ssr.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/shop/src/environments/environment.ts",
                  "with": "apps/shop/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        }
      }
    },
    "shop-e2e": {
      "root": "apps/shop/e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "shop:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "shop:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/shop/e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "functions/dist/server",
            "main": "apps/shop/src/main-ssr.ts",
            "tsConfig": "apps/shop/src/tsconfig.app-ssr.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/shop/src/environments/environment.ts",
                  "with": "apps/shop/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        }
      }
    },
    "dashboard": {
      "root": "apps/dashboard",
      "sourceRoot": "apps/dashboard/src",
      "projectType": "application",
      "prefix": "jfs",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss",
          "changeDetection": "OnPush"
        },
        "@schematics/angular:directive": {
          "flat": false
        },
        "@schematics/angular:pipe": {
          "flat": false
        },
        "@schematics/angular:service": {
          "flat": false
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "../dist/public/dashboard",
            "index": "apps/dashboard/src/index.html",
            "main": "apps/dashboard/src/main.ts",
            "polyfills": "apps/dashboard/src/polyfills.ts",
            "tsConfig": "apps/dashboard/src/tsconfig.app.json",
            "assets": [
              "apps/dashboard/src/favicon.ico",
              "apps/dashboard/src/assets",
              "apps/dashboard/src/manifest.json",
              {
                "glob": "**/*",
                "input": "node_modules/tinymce/themes/silver",
                "output": "/themes/silver"
              },
              {
                "glob": "**/*",
                "input": "node_modules/tinymce/skins/ui/oxide",
                "output": "/skins/ui/oxide"
              },
              {
                "glob": "**/*",
                "input": "node_modules/tinymce/skins/content/default",
                "output": "/skins/content/default"
              }
            ],
            "styles": ["apps/dashboard/src/styles.scss"],
            "scripts": ["./node_modules/tinymce/tinymce.min.js"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/dashboard/src/environments/environment.ts",
                  "with": "apps/dashboard/src/environments/environment.prod.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "10mb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dashboard:build",
            "port": 5000
          },
          "configurations": {
            "production": {
              "browserTarget": "dashboard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dashboard:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/dashboard/src/test.ts",
            "polyfills": "apps/dashboard/src/polyfills.ts",
            "tsConfig": "apps/dashboard/src/tsconfig.spec.json",
            "karmaConfig": "apps/dashboard/src/karma.conf.js",
            "styles": ["apps/dashboard/src/styles.scss"],
            "scripts": [],
            "assets": [
              "apps/dashboard/src/favicon.ico",
              "apps/dashboard/src/assets",
              "apps/dashboard/src/manifest.json"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/dashboard/src/tsconfig.app.json",
              "apps/dashboard/src/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "dashboard-e2e": {
      "root": "apps/dashboard/e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "dashboard:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dashboard:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/dashboard/e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "functions/dist/server",
            "main": "apps/dashboard/src/main-ssr.ts",
            "tsConfig": "apps/dashboard/src/tsconfig.app-ssr.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/dashboard/src/environments/environment.ts",
                  "with": "apps/dashboard/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        }
      }
    }
  },
  "defaultProject": "shop"
}
